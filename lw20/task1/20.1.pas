PROGRAM GraphicPrinting(INPUT, OUTPUT);
CONST
  Width = 5;
  Size = 25;
TYPE
  MatrixSize = SET OF 1..Size;
VAR
  Ch: CHAR;
  Count: INTEGER;
  Matrix: MatrixSize;
BEGIN
  Count := 1;
  Matrix := [];
  READ(Ch);
  CASE Ch OF
    'A': Matrix := [2, 3, 4, 6, 10, 11, 12, 13, 14, 15, 16, 20, 21, 25];
    'B': Matrix := [1, 2, 3, 4, 6, 10, 11, 12, 13, 14, 16, 20, 21, 22, 23, 24, 25];
    'C': Matrix := [2, 3, 4, 6, 10, 11, 16, 20, 22, 23, 24];
    'D': Matrix := [1, 2, 3, 4, 6, 10, 11, 15, 16, 20, 21, 22, 23, 24];
    'E': Matrix := [1, 2, 3, 4, 5, 6, 11, 12, 13, 14, 15, 16, 21, 22, 23, 24, 25];
    'F': Matrix := [1, 2, 3, 4, 5, 6, 11, 12, 13, 14, 15, 16, 21];
    'G': Matrix := [1, 2, 3, 4, 5, 6, 11, 14, 15, 16, 20, 21, 22, 23, 24, 25];
    'H': Matrix := [1, 5, 6, 10, 11, 12, 13, 14, 15, 16, 20, 21, 25];
    'I': Matrix := [1, 2, 3, 4, 5, 8, 13, 18, 21, 22, 23, 24, 25];
    'J': Matrix := [2, 3, 4, 8, 13, 16, 18, 21, 22, 23];
    'K': Matrix := [1, 5, 6, 9, 11, 12, 13, 16, 19, 21, 25];
    'L': Matrix := [1, 6, 11, 16, 21, 22, 23, 24, 25];
    'M': Matrix := [1, 5, 6, 7, 9, 10, 11, 13, 15, 16, 20, 21, 25];
    'N': Matrix := [1, 5, 6, 7, 10, 11, 13, 15, 16, 19, 20, 21, 25];
    'O': Matrix := [2, 3, 4, 6, 10, 11, 15, 16, 20, 22, 23, 24];
    'P': Matrix := [1, 2, 3, 4, 6, 10, 11, 12, 13, 14, 16, 21];
    'Q': Matrix := [1, 2, 3, 4, 6, 9, 11, 14, 16, 17, 18, 19, 25];
    'R': Matrix := [1, 2, 3, 4, 5, 6, 10, 11, 12, 13, 14, 15, 16, 19, 21, 25];
    'S': Matrix := [2, 3, 4, 5, 6, 12, 13, 14, 20, 21, 22, 23, 24];
    'T': Matrix := [1, 2, 3, 4, 5, 8, 13, 18, 23];
    'U': Matrix := [1, 5, 6, 10, 11, 15, 16, 20, 22, 23, 24];
    'V': Matrix := [1, 5, 6, 10, 11, 15, 17, 19, 23];
    'W': Matrix := [1, 5, 6, 10, 11, 15, 16, 18, 20, 22, 24];
    'X': Matrix := [1, 5, 7, 9, 13, 17, 19, 21, 25];
    'Y': Matrix := [1, 5, 7, 9, 13, 18, 23];
    'Z': Matrix := [1, 2, 3, 4, 5, 9, 13, 17, 21, 22, 23, 24, 25];
    '1': Matrix := [3, 7, 8, 11, 13, 18, 23];
    '2': Matrix := [2, 3, 4, 6, 10, 14, 18, 21, 22, 23, 24, 25];
    '3': Matrix := [1, 2, 3, 4, 10, 13, 14, 20, 21, 22, 23, 24];
    '4': Matrix := [3, 4, 7, 9, 11, 12, 13, 14, 15, 19, 24];
    '5': Matrix := [1, 2, 3, 4, 6, 11, 12, 13, 14, 20, 21, 22, 23, 24];
    '6': Matrix := [2, 3, 4, 6, 11, 12, 13, 14, 16, 20, 21, 22, 23, 24];
    '7': Matrix := [2, 3, 4, 5, 9, 13, 18, 23];
    '8': Matrix := [2, 3, 4, 6, 10, 12, 13, 14, 16, 20, 22, 23, 24];
    '9': Matrix := [2, 3, 4, 6, 10, 12, 13, 14, 15, 20, 22, 23, 24]
  FOR Count := 1 TO Size
  DO
    BEGIN
      IF Count IN Matrix
      THEN
        WRITE('X')    
      ELSE
        WRITE(' ');
      IF (Count MOD Width = 0)
      THEN
        WRITELN
    END; 
END.
                            
